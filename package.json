  "keywords": [
    "deserialize",
    "json",
    "parser",
    "xml"
  ],
  "license": "MIT",
  "main": "./from-xml.js",
  "module": "./dist/from-xml.mjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kawanet/from-xml.git"
  },
  "scripts": {
    "build": "make",
    "clean": "make clean",
    "fixpack": "fixpack",
    "prepack": "make clean all test",
    "test": "make test",
    "test-browser": "make && open test/test.html"
  },
  "type": "commonjs",
  "types": "from-xml.d.ts"
}
